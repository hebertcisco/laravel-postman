{
    "name": "yasin_tgh/laravel-postman",
    "description": "A powerful package to automatically generate Postman collections from your Laravel routes with intelligent organization and rich documentation capabilities.",
    "type": "library",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "symfony/console": "^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "YasinTgh\\LaravelPostman\\": "src/"
        }
    },
    "authors": [
        {
            "name": "yasin taghavi",
            "email": "mrtqvi@yahoo.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "YasinTgh\\LaravelPostman\\PostmanServiceProvider"
            ]
        }
    },
    "license": "MIT"
}